{
  "extends": "solhint:recommended",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "compiler-version": ["error", "0.6.12"],
    "code-complexity": ["warn", 7],
    "function-max-lines": ["warn", 50],
    "constructor-syntax": "warn",
    "modifier-name-mixedcase": "warn",
    "ordering": "warn"
  }
}
